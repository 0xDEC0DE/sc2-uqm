default:
	./build.sh sc2

%.rc.o:
	@DIR=$(dir $@); \
	if [ ! -d $$DIR ]; then \
		mkdir -p "$$DIR"; \
	fi
	$(WINDRES) --include-dir $(dir $<) -o $@ $<

%.o:
	@DIR=$(dir $@); \
	if [ ! -d $$DIR ]; then \
		mkdir -p "$$DIR"; \
	fi
	$(COMPILE) -o $@ -c $(CFLAGS) $<

target-%:
	$(COMPILE) -o $* $^ $(LDFLAGS)

-include $(DEPEND_FILE)

