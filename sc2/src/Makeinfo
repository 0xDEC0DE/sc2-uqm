uqm_SUBDIRS="sc2code res"

case "$HOST_SYSTEM" in
	ARMV5|GCCE|WINSCW)
		;;
	*)
		uqm_SUBDIRS="$uqm_SUBDIRS getopt"
		;;
esac

case "$HOST_SYSTEM" in
	Darwin)
		uqm_SUBDIRS="$uqm_SUBDIRS darwin"
		;;
esac

if [ "$uqm_HAVE_REGEX" = 0 ]; then
	uqm_SUBDIRS="$uqm_SUBDIRS regex"
fi

uqm_CFILES="options.c port.c starcon2.c"

test_CFILES=test.c

